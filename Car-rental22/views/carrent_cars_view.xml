<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record model="ir.ui.view" id="carrental_cars_view_form">
    <field name="name">carrental_cars_view_form</field>
   <field name="model">carrental.cars</field>
    <field name="arch" type="xml">
      <form string="cars">
		<sheet>
		 <group col="4">
               <group colspan="2">
      	     
      	  	<field name="cid" placeholder="car_number"/>
      	  	<field name="owner_name" placeholder="owner_name"/>
      	  	<field name="make" placeholder="make"/>
      	  	<field name="year_of_manufacture" placeholder="year_of_manufacture"/>
      	  	
      	  	
       	
       	</group>
       	</group>
       	</sheet>
    </form>
    </field>
  </record>

 </odoo>
